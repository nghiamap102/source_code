    "dateformat": "^5.0.2",
    "emotion-theming": "^11.0.0",
    "formik": "^2.2.9",
    "framer-motion": "^4.0.3",
    "json-server": "^0.17.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "11.1.2",
    "query-string": "^7.1.1",
    "react": "17.0.2",
    "react-datepicker": "^4.3.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "17.0.2",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
