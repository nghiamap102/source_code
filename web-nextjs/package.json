{
  "name": "nutri-asia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn run i18n:compile",
    "dev": "next dev",
    "prebuild": "npm run i18n:compile",
    "storybook": "start-storybook -p 6006",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "test:coverage": "npm test -- --coverage --watchAll=false --verbose=true",
    "test:push": "npm test -- --coverage --watchAll=false --verbose=false",
    "lint": "next lint",
    "lint:write": "next lint --cache --fix",
    "i18n:extract": "formatjs extract 'pages/**/*.ts*' 'components/**/*.ts*' --out-file lang/en.json",
    "i18n:compile": "formatjs compile-folder lang compiled-lang",
    "build-storybook": "build-storybook",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "pre-commit": "npm run prettier && git add . && npm run lint",
    "export": "next build && next export",
    "pre-push": "npm run test:push",
    "mock": "json-server --watch \"./src/apiMock/db.json\" --port 3004 --routes \"./src/apiMock/routes.json\""
  },
  "dependencies": {
